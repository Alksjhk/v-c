{
  "version": 2,
  "buildCommand": "npm run build:vercel",
  "installCommand": "npm install && cd clinet && npm install",
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/dist/app.js"
    },
    {
      "src": "/uploads/(.*)",
      "dest": "/dist/app.js"
    },
    {
      "src": "/health",
      "dest": "/dist/app.js"
    },
    {
      "src": "/sse/(.*)",
      "dest": "/dist/app.js"
    },
    {
      "src": "/(.*)",
      "dest": "/dist/app.js"
    }
  ],
  "env": {
    "NODE_ENV": "production"
  }
}
